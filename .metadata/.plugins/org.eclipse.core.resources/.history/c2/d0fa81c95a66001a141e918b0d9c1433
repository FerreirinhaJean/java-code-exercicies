package applicantion;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintWriter;
import java.util.Scanner;

public class Main {

	static BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
	static PrintWriter out = new PrintWriter(System.out);

	public static void main(String[] args) throws IOException {

		String paises;
		while ((paises = in.readLine()) != null) {
			switch (paises) {
			case "brasil":
			case "portugal":
				out.println("Feliz Natal!");
				break;
			case "alemanha":
				out.println("Frohliche Weihnachten!");
				break;
			case "austria":
				out.println("Frohe Weihnacht!");
				break;
			case "coreia":
				out.println("Chuk Sung Tan!");
				break;
			case "espanha":
			case "argentina":
			case "chile":
			case "mexico":
				out.println("Feliz Navidad!");
				break;
			case "grecia":
				out.println("Kala Christougena!");
				break;
			case "estados-unidos":
			case "inglaterra":
			case "australia":
			case "antardida":
			case "canada":
				out.println("Merry Christmas!");
				break;
			case "suecia":
				out.println("God Jul!");
				break;
			case "irlanda":
				out.println("Nollaig Shona Dhuit!");
				break;
			case "belgica":
				out.println("Zalig Kerstfeest!");
				break;
			case "italia":
			case "libia":
				out.println("Buon Natale!");
				break;
			case "siria":
			case "marrocos":
				out.println("Milad Mubarak!");
				break;
			case "japao":
				out.println("Merii Kurisumasu!");
				break;
			case "turquia":
				out.println("Mutlu Noeller");
				break;
			default:
				out.println("--- NOT FOUND ---");
				break;
			}
		}

		out.close();
	}

}
